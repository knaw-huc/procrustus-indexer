                  <valItem ident="pages"><desc>pages</desc></valItem>
                  <valItem ident="words"><desc>words</desc></valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="msDesc" mode="change" module="msdescription">
            <attList>
              <attDef ident="xml:id" mode="change" usage="req"/>
              <attDef ident="xml:lang" mode="change" usage="req">
                <defaultVal>de</defaultVal>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="name" mode="change" module="core">
            <attList>
              <attDef ident="type" mode="change" usage="rec">
                <defaultVal>unknown</defaultVal>
                <valList type="closed" mode="replace">
                  <valItem ident="person"/>
                  <valItem ident="place"/>
                  <valItem ident="org"/>
                  <valItem ident="unknown"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="objectDesc" mode="change" module="msdescription">
            <attList>
              <attDef ident="form" mode="change" usage="req">
                <defaultVal>codex</defaultVal>
                <valList type="closed" mode="replace">
                  <valItem ident="codex"><desc>a bound codex</desc></valItem>
                  <valItem ident="fascicle"><desc>part of a bound codex with its own history</desc></valItem>
                  <valItem ident="leaf"><desc>a loose leaf</desc></valItem>
                  <valItem ident="scroll"><desc>a scroll</desc></valItem>
                  <valItem ident="other"><desc>any other format</desc></valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="person" mode="change" module="namesdates">
            <attList>
              <attDef ident="sex" mode="change" usage="req">
                <defaultVal>0</defaultVal>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="ref" mode="change" module="core">
            <attList>
              <attDef ident="type" mode="change" usage="rec">
                <valList type="semi" mode="replace">
                  <valItem ident="biblical"><desc>certain type of reference, should be accompagnied by cRef</desc></valItem>
                  <valItem ident="classical"><desc>certain type of reference, should be accompagnied by cRef</desc></valItem>
                  <valItem ident="medieval"><desc>certain type of reference, should be accompagnied by cRef</desc></valItem>
                  <valItem ident="altMs"><desc>reference to a manuscript that belongs to another institution other than the owner of the described manuscript</desc></valItem>
                  <valItem ident="mss"><desc>reference to a manuscript that belongs to the same institution as the described manuscript</desc></valItem>
                  <valItem ident="inkunabeln"/>
                  <valItem ident="drucke"/>
                  <valItem ident="vd16"/>
                  <valItem ident="vd17"/>
                  <valItem ident="pdf"/>
                  <valItem ident="purl"/>
                  <valItem ident="urn"/>
                  <valItem ident="doi"/>
                  <valItem ident="crossRef"/>
                  <valItem ident="repertorium"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="region" mode="change" module="namesdates">
            <attList>
              <attDef ident="type" mode="change" usage="rec">
                <defaultVal>unknown</defaultVal>
                <valList type="closed" mode="replace">
                  <valItem ident="parish"/>
                  <valItem ident="county"/>
                  <valItem ident="compass"/>
                  <valItem ident="geog"/>
                  <valItem ident="state"/>
                  <valItem ident="unknown"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <!-- compare values of <author>, and copy any changes to the other place -->
          <elementSpec ident="rs" module="core" mode="change">
            <attList>
              <attDef ident="role" mode="change" usage="rec">
                <defaultVal>author</defaultVal>
                <valList type="semi" mode="replace">
                  <valItem ident="author"><desc>author of a work - FRBR work</desc></valItem>
                  <valItem ident="translator"><desc>translator of a work - FRBR expression</desc></valItem>
                  <valItem ident="commentator"><desc>commentator of a work - FRBR manifestation</desc></valItem>
                  <valItem ident="editor"><desc>editor of a work - FRBR manifestation/item</desc></valItem>
                  <valItem ident="other"><desc>unspecified</desc></valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="supplied" mode="change" module="transcr">
            <attList>
              <attDef ident="reason" mode="change" usage="rec">
                <defaultVal>unknown</defaultVal>
                <valList type="closed" mode="replace">
                  <valItem ident="omitted"/>
                  <valItem ident="illegible"/>
                  <valItem ident="damage"/>
                  <valItem ident="unknown"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="supportDesc" mode="change" module="msdescription">
            <attList>
              <attDef ident="material" mode="change" usage="req">
                <defaultVal>unknown</defaultVal>
                <valList type="semi" mode="replace">
                  <valItem ident="perg">
                    <desc xml:lang="en">parchment</desc>
                    <desc xml:lang="de">Pergament</desc>
                  </valItem>
                  <valItem ident="chart">
                    <desc xml:lang="en">paper</desc>
                    <desc xml:lang="de">Papier</desc>
                  </valItem>
                  <valItem ident="mixed">
                    <desc xml:lang="en">mixture of any other materials than paper, papyrus, and parchment</desc>
                    <desc xml:lang="de">Mischung aus anderen Materialien als Papier, Papyrus und Pergment</desc>
                  </valItem>
                  <valItem ident="papyrus"/>
                  <valItem ident="papyrus_perg">
                    <desc xml:lang="en">mixture of papyrus and parchment</desc>
                    <desc xml:lang="de">Mischung aus Papyrus und Pergment</desc>
                  </valItem>
                  <valItem ident="papyrus_chart">
                    <desc xml:lang="en">mixture of papyrus and paper</desc>
                    <desc xml:lang="de">Mischung aus Papyrus und Papier</desc>
                  </valItem>
                  <valItem ident="perg_chart">
                    <desc xml:lang="en">mixture of paper and parchment</desc>
                    <desc xml:lang="de">Mischung aus Papier und Pergment</desc>
                  </valItem>
                  <valItem ident="unknown">
                    <desc xml:lang="en">unknown</desc>
                    <desc xml:lang="de">unbekannt</desc></valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>

          <elementSpec ident="textLang" mode="change" module="msdescription">
            <attList>
              <attDef ident="mainLang" mode="change" usage="req"/>
            </attList>
          </elementSpec>

          <classSpec ident="att.dimensions" mode="change" type="atts">
            <attList>
              <attDef ident="precision" mode="delete"/>
              <attDef ident="unit" mode="change" usage="opt">
                <defaultVal>cm</defaultVal>
                <valList type="semi" mode="replace">
                  <valItem ident="chars"><desc>written characters</desc></valItem>
                  <valItem ident="leaves"><desc>leaves</desc></valItem>
                  <valItem ident="lines"><desc>lines</desc></valItem>
                  <valItem ident="cm"><desc>centimetres</desc></valItem>
                  <valItem ident="mm"><desc>millimetres</desc></valItem>
                  <valItem ident="pages"><desc>pages</desc></valItem>
                  <valItem ident="words"><desc>words</desc></valItem>
                </valList>
              </attDef>
            </attList>
          </classSpec>

          <!-- constrain date attributes -->

          <elementSpec ident="date" mode="change" module="core">
            <content>
              <s:pattern name="date_values">
                <s:rule context="tei:date">
                  <s:assert test="@when or (@notAfter and @notBefore) or (@from and @to)">You must provide either @when or @to/@from, or @notAfter/@notBefore.</s:assert>
                </s:rule>
              </s:pattern>
            </content>
          </elementSpec>

          <elementSpec ident="origDate" mode="change" module="msdescription">
            <content>
              <s:pattern name="date_values">
                <s:rule context="tei:origDate">
                  <s:assert test="@when or (@notAfter and @notBefore) or (@from and @to)">You must provide either @when or @to/@from, or @notAfter/@notBefore.</s:assert>
                </s:rule>
              </s:pattern>
            </content>
          </elementSpec>

          <!-- remove optional global attribute we don't want to use -->

          <classSpec ident="att.global" type="atts" mode="change" module="tei">
            <attList>
              <attDef ident="rendition" mode="delete"/>
            </attList>
          </classSpec>


          <!-- remove stupid (non-mandatory) elements we dont want in this place -->
          <!-- remove option of change within history -->
          <!-- for what reason has this been removed? it sounds reasonable to me to have <change>-->
          <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0" module="msdescription"
            ident="recordHist" mode="change">
            <content>
              <rng:choice>
                <rng:oneOrMore>
                  <rng:ref name="model.pLike"/>
                </rng:oneOrMore>
                <rng:ref name="source"/>
              </rng:choice>
            </content>
          </elementSpec>


          <!-- HERE BE DIRTY STUFF WHICH SHOULD BE IN ITS OWN NAMESPACE 
     BUT WHICH IS ALREADY SCHEDULED FOR INCLUSION IN NEXT P5 RELEASE-->


          <!-- change wording of desc to be less confusing -->
          <elementSpec ident="surrogates" mode="change" module="msdescription">
            <desc>contains information about any non-digital representations of the manuscript being described which may exist in the holding institution or elsewhere. </desc>
          </elementSpec>

        </schemaSpec>
      </div>
    </body>
  </text>
</TEI>
